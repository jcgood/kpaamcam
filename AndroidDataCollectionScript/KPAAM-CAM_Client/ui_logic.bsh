    tempAnsLabel=ansLabelFstPart+ansLabelMidPart+ansLabelSndPart;
    switch(typeflag){
    	case "non":
    		startNewAnswerBasic(current_start_time);
    	break;
    	case "chk":
    		startNewAnswerChk();
    	break;
    	case "lan":
    		startNewAnswerLangIntro();
