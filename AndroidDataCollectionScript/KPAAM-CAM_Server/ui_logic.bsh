}

nameCamCaseConverter(String originName){
    String camCase=null;
    String [] originNameSplit=originName.split("\\s+");
    StringBuffer sb=new StringBuffer();
    for (part : originNameSplit){
        if(part.length()>1){
            sb.append(Character.toUpperCase(part.charAt(0))).append(part.substring(1));
        }
        else{
            sb.append(Character.toUpperCase(part.charAt(0)));
        }
    }
    camCase=sb.toString().trim();
    return camCase;
}

escapeSpecialCharInQuery(String queryKw){
	String specialPtn="[\\[\\]{};~>*%\\&\\?,\\!\\$\\|]+";
    Pattern pattern = Pattern.compile(specialPtn);
    if(pattern.matcher(queryKw).find()){
        return "";
    }
    queryKw=queryKw.trim().replaceAll("'","''");
    queryKw=queryKw.replaceAll("\\\\","\\\\\\\\");
    return queryKw;
}

