/*
** generate an (x,y,z,) format IN condition for queries
*/
buildQuerySetCondition(ArrayList itemList){
	StringBuilder sb=new StringBuilder();
	sb.append("(");
	for (String s: itemList){
		String str="'"+s+"'";
		if(sb.length()>1){
			sb.append(",");
		}
		sb.append(str);
	}
	sb.append(")");
	return sb.toString();
}

