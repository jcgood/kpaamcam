                break;
            }
        }
    }
}

storeQuestionChoice(String quesOptCountStr, String langVerId){
    int quesOptCount=Integer.parseInt(quesOptCountStr);
   
    ArrayList tempOptText=new ArrayList();
    for(int i=0;i<quesOptCount;i++){
        if(hasView("questionChoice/questionChoiceInfo/optc" + i)){
            String tempQuesContent=getFieldValue("questionChoice/questionChoiceInfo/optc" + i);
            tempOptText.add(tempQuesContent);
        }
    }

    if(tempOptText==null||tempOptText.size()<2){
        showWarning("No option added","No data added, you must add at least two options for this question");
        return;
    }

    ArrayList dupCheck=checkDupItem(tempOptText);
    if(dupCheck==null){
        storeQuestionChoiceNoDup(tempOptText,langVerId);
    }
    else{
        showWarning("Duplicate option added","No duplicate option allowed:"+dupCheck.get(0)+"\n Please correct the option");
        return;
    }
}

checkDupItem(ArrayList elmList){
    HashSet dupCheck=new HashSet();
    ArrayList dupList=new ArrayList();
    Boolean dup=false;
    for(elm: elmList){
        if(dupCheck.contains(elm)){
            dup=true;
            dupList.add(elm);
            break;
        }
        else{
            dupCheck.add(elm);
        }
    }
    if(!dup){
        return null;
    }
    else{
        return dupList;
    }
    return null;
}

storeQuestionChoiceNoDup(ArrayList optList, String langVerId){
    newQuestionOptionList.put(langVerId,optList);
    cancelTabGroup("questionChoice",true);
}

