updateQuesLang(ArrayList selLangList){
    if(question_id==null || oldLangList==null || oldLangList.isEmpty()){
        return;
    }
    tempOldLangList=new ArrayList();
    for(old:oldLangList){
        ArrayList tempLang=new ArrayList();
        tempLang.add(old.get(1));
        tempLang.add(old.get(2));
        tempLang.add(old.get(3));
        tempOldLangList.add(tempLang);
    }
    compIndex=new ArrayList();
    compIndex.add(1);
    compIndex.add(2);
    Hashtable quesLangChange=listChange(selLangList,tempOldLangList,0,compIndex);
    if(quesLangChange.containsKey("A")){
		tempNewLang=quesLangChange.get("A");
		    for(lang: tempNewLang){
		        quesLangVer=createAttributeList();
		        quesLangVer.add(createEntityAttribute("QuestionUuid", null, null, question_id, null));
		        quesLangVer.add(createEntityAttribute("QuesLangUuid", null, null, lang.get(0), null));
		        quesLangVer.add(createEntityAttribute("QuesContent", lang.get(2), null, lang.get(1), null));
		        saveArchEnt(null, "QuestionLang", null, quesLangVer);
		    }
    }
    
    if(quesLangChange.containsKey("D")){
    	tempDelLang=quesLangChange.get("D");
	    for(delLang: tempDelLang){
	        for(oldLang:oldLangList){
	            if(delLang.get(0).equals(oldLang.get(1))){
	                deleteArchEnt(oldLang.get(0));
	            }
	        }
	    }
    }
    
    if(quesLangChange.containsKey("U")){
    	tempUpdLang=quesLangChange.get("U");
	    for(uLang: tempUpdLang){
	        for(oldLang:oldLangList){
	            if(uLang.get(0).equals(oldLang.get(1))){
	                quesLangVer=createAttributeList();
	                quesLangVer.add(createEntityAttribute("QuestionUuid", null, null, question_id, null));
	                quesLangVer.add(createEntityAttribute("QuesLangUuid", null, null, uLang.get(0), null));
	                quesLangVer.add(createEntityAttribute("QuesContent", uLang.get(2), null, uLang.get(1), null));
	                saveArchEnt(oldLang.get(0), "QuestionLang", null, quesLangVer);
	            }
	        }
	    }
    }
    
    showToast("Question language updated"); 
}

updateQuesChoice(ArrayList selLangList){
    if(question_id==null){
        return;
    }
    if(isNull(oldQuestionOptionList) && isNull(newQuestionOptionList)){
    	return;
    }

    checkQuesOptUpdate(selLangList);
}

checkQuesOptUpdate(ArrayList selLangList){
    ArrayList oldKeySet=new ArrayList();
    if(!isNull(oldQuestionOptionList)){
    	 oldKeySet.addAll(oldQuestionOptionList.keySet());
    }
   
     
    getNewLangKey(selLangList);
    ArrayList newKeySet=new ArrayList();
    if(!isNull(newQuestionOptionList)){
    	newKeySet.addAll(newQuestionOptionList.keySet());
    }
    Hashtable keyDiff=new Hashtable();

    keyDiff=listChange(newKeySet,oldKeySet);

    HashMap langDiff=addOrDelQuesOptListByLang(keyDiff);
    ArrayList equalKey=getEquKey(oldKeySet,newKeySet);
    if(langDiff==null||langDiff.isEmpty()){
        checkQuesOptContentUpdate(equalKey);
    }
    else if(langDiff.containsKey("PA")){
        updateQuesChoiceAdd(langDiff.get("PA"));
        checkQuesOptContentUpdate(equalKey);
    }
    else if(langDiff.containsKey("PD")){
        updateQuesChoiceDel(langDiff.get("PD"));
        checkQuesOptContentUpdate(equalKey);
    }
    else{
        updateQuesChoiceDel(langDiff.get("D"));
        updateQuesChoiceAdd(langDiff.get("A"));
        checkQuesOptContentUpdate(equalKey);
    }
}

getNewLangKey(ArrayList selLangList){
    ArrayList existLang=new ArrayList();
    for(lang: selLangList){
        existLang.add(lang.get(0));
    } 

    ArrayList toRmvKey=new ArrayList();
    for(String key: newQuestionOptionList.keySet()){
        if(!existLang.contains(key)){
            toRmvKey.add(key);
        }
    }
    for(rmvKey: toRmvKey){
        newQuestionOptionList.remove(rmvKey);
    }
}

getEquKey(ArrayList originList, ArrayList newList){
    ArrayList tempList=new ArrayList();
    tempList.addAll(originList);
    ArrayList tempNewList=new ArrayList();
    tempNewList.addAll(newList);
    tempList.retainAll(tempNewList);
    return tempList;
}

addOrDelQuesOptListByLang(Hashtable keyDiff){
    HashMap updateList=new HashMap();
    if(keyDiff.containsKey("EQUAL")){
        return null;
    }
    else if(keyDiff.containsKey("PUREADD")){
        ArrayList langAdded=keyDiff.get("PUREADD");
        ArrayList pureAddList=new ArrayList();
        for(lang:langAdded){
            ArrayList tempLangOpt=newQuestionOptionList.get(lang);
            HashMap pureAddMap=new HashMap();
            pureAddMap.put(lang,tempLangOpt);
            pureAddList.add(pureAddMap);
        }
        updateList.put("PA",pureAddList);
    }
    else if(keyDiff.containsKey("PUREDELETE")){
        ArrayList langDel=keyDiff.get("PUREDELETE");
        ArrayList pureDelList=new ArrayList();
        for(lang:langDel){
            ArrayList tempLangOpt=oldQuestionOptionList.get(lang);
            HashMap pureDelMap=new HashMap();
            pureDelMap.put(lang,tempLangOpt);
            pureDelList.add(pureDelMap);
        }
        updateList.put("PD",pureDelList);
    }
    else{
        showWarning("ADD","ADD");
        ArrayList langAdded=keyDiff.get("ADD");
        ArrayList addList=new ArrayList();
        for(lang:langAdded){
            ArrayList tempLangOpt=newQuestionOptionList.get(lang);
            HashMap addMap=new HashMap();
            addMap.put(lang,tempLangOpt);
            addList.add(addMap);
        }
        updateList.put("A",addList);

        ArrayList langDel=keyDiff.get("DELETE");
        ArrayList delList=new ArrayList();
        for(lang:langDel){
            ArrayList tempLangOpt=oldQuestionOptionList.get(lang);
            HashMap delMap=new HashMap();
            delMap.put(lang,tempLangOpt);
            delList.add(delMap);
        }
        updateList.put("D",delList);
    }
    return updateList;
}

updateQuesChoiceAdd(ArrayList pureAddList){
    if(pureAddList==null|| pureAddList.isEmpty()){
        return;
    }
    for(lang:pureAddList){
        for(String key: lang.keySet()){
            langKey=key;
            ArrayList langOptList=newQuestionOptionList.get(langKey);
            saveQuesChoiceByLang(question_id, langKey,langOptList);
        }    
    }
}

updateQuesChoiceDel(ArrayList pureDelList){
    if(pureDelList==null|| pureDelList.isEmpty()){
        return;
    }
    ArrayList tempOptIdList=new ArrayList();
    for(lang : pureDelList){//lang is a hashmap
        for(String key: lang.keySet()){
            lang=key;
            ArrayList tempOptList=oldQuestionOptionList.get(lang);
            for(opt: tempOptList){
                String [] idSplit=opt.split(":");
                tempOptIdList.add(idSplit[0]);
            }
        }
    }
    delOldQuesChoice(tempOptIdList);
}

delOldQuesChoice(ArrayList delIdList){
    if(delIdList==null){
        return;
    }
    for(int i=0;i<delIdList.size();i++){
        deleteArchEnt(delIdList.get(i));
    }
}

checkQuesOptContentUpdate(ArrayList equKeyList){
    if(equKeyList==null || equKeyList.size()==0){
        return;
    }
    HashMap contentUpdate=new HashMap();
    for(int i=0;i<equKeyList.size();i++){
        String key=equKeyList.get(i);
        HashMap tempOldOptList=extractOldOptByLang(key);
        ArrayList tempOldOptContentList=new ArrayList();
        tempOldOptContentList.addAll(tempOldOptList.keySet());
        ArrayList tempNewOptList=new ArrayList();
        tempNewOptList.addAll(newQuestionOptionList.get(key));
        //to here when del
        Hashtable optContentDiff=listChange(tempNewOptList,tempOldOptContentList);
    
        if(optContentDiff.containsKey("PUREDELETE")){
            ArrayList toDelOptList=prepDelOpt(optContentDiff,tempOldOptList);
            optContentDiff.put("PUREDELETE",toDelOptList);
        }
        
        else if(optContentDiff.containsKey("DELETE")){
            ArrayList toDelOptList=prepDelOpt(optContentDiff,tempOldOptList);
            optContentDiff.put("DELETE",toDelOptList);
        }

        contentUpdate.put(key, optContentDiff);
    }
    quesOptContentUpdate(contentUpdate);
}

extractOldOptByLang(String key){
    ArrayList tempOpt=oldQuestionOptionList.get(key);
    HashMap extedOldOptList=new HashMap();
    for(int i=0;i<tempOpt.size();i++){
        String [] idSplit= tempOpt.get(i).split(":");
        ArrayList tempOptItem=new ArrayList();
        extedOldOptList.put(idSplit[1],idSplit[0]);
    }
    return extedOldOptList;
}

prepDelOpt(Hashtable diffMap, HashMap oldOptMap){
    ArrayList toDelOptList=new ArrayList();
    for(String diffKey: diffMap.keySet()){
        if(diffKey.equals("PUREDELETE")||diffKey.equals("DELETE")){
            ArrayList diffList=diffMap.get(diffKey);
            for(optCtn:diffList){
                if(oldOptMap.containsKey(optCtn)){
                    toDelOptList.add(oldOptMap.get(optCtn));
                }
            }

        }
    }
    return toDelOptList;
}

quesOptContentUpdate(HashMap optUpdate){
    for(String key:optUpdate.keySet()){
        Hashtable diff=optUpdate.get(key);
        //tohere
        if(diff.containsKey("PUREADD")){
            ArrayList addList=diff.get("PUREADD");
            saveQuesChoiceByLang(question_id, key, addList);
        }
        else if(diff.containsKey("PUREDELETE")){
            ArrayList delList=diff.get("PUREDELETE");
            delOldQuesChoice(delList);
        }
        else if(diff.containsKey("ADD")){
            ArrayList delList=diff.get("DELETE");
            delOldQuesChoice(delList);
            ArrayList addList=diff.get("ADD");
            saveQuesChoiceByLang(question_id, key, addList);
        }
    }
}

