/*
** generate sql keyword without special characters
*/

escapeSpecialCharInQuery(String queryKw){
	String specialPtn="[\\[\\]{};~>*%\\&\\?,\\!\\$\\|]+";
	Pattern pattern = Pattern.compile(specialPtn);
    if(pattern.matcher(queryKw).find()){
        return "";
    }
	queryKw=queryKw.trim().replaceAll("'","''");
    queryKw=queryKw.replaceAll("\\\\","\\\\\\\\");
    return queryKw;
}

